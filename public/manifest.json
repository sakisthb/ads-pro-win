    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", ".csv", ".xlsx"]
        }
      ]
    }
  },
  
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "application/json": [".json"]
      }
    }
  ],
  
  "protocol_handlers": [
    {
      "protocol": "web+adspro",
      "url": "/campaign?id=%s"
    }
  ],
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "prefer_related_applications": false,
  "related_applications": [],
  
  "iarc_rating_id": "",
  
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
}
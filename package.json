  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:ws": "concurrently \"npm run dev\" \"npm run ws:dev\"",
    "ws:dev": "nodemon scripts/start-websocket-server.js",
    "ws:start": "node scripts/start-websocket-server.js",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:full": "concurrently \"npm run start\" \"npm run ws:start\"",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env ANALYZE=true next build && next start",
    "build:analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --silent",
    "test:ui": "jest --testPathPattern=components",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:ci && npm run test:e2e",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:studio": "prisma studio",
    "security:audit": "npm audit --audit-level=high",
    "test:security": "npm run security:audit",
    "test:api": "jest --testPathPattern=api",
